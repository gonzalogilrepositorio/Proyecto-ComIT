@model IEnumerable<Proyecto.Models.Property>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
         <th>Codigo</th>
                      
        <th>
            Email
        </th>
      
        <th>
           Titulo
        </th>       
        <th>
            Direccion
        </th>
            
        <th>
            @Html.DisplayNameFor(model => model.TiempoRestante)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FechaPublicacion)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Id)
        </td>
      
        <td>
            @Html.DisplayFor(modelItem => item.User.Email)
        </td>
       
        <td>
            @Html.DisplayFor(modelItem => item.Titulo)
        </td>
        
        <td>
            @Html.DisplayFor(modelItem => item.Direccion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FechaPublicacion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TiempoRestante)
        </td>
       
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>

    </tr>
    

}

</table>
